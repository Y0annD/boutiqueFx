<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.y0annd.boutique.app.controller.SaisieNomVilleController">
   <children>
      <HBox fillHeight="false" spacing="10.0">
         <children>
            <Label fx:id="lblNomVille" text="Nom de ville" />
            <TextField fx:id="txtVille" HBox.hgrow="ALWAYS" />
            <Button id="ville.valider" fx:id="btnValidate" mnemonicParsing="false" onAction="#onValider" text="Valider" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <Label fx:id="lblCodePostal" text="Code postal" textAlignment="CENTER">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin></Label>
   </children>
</VBox>
