<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>


<VBox spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="fr.y0annd.boutique.app.controller.BackgroundTaskController">
   <children>
      <Button fx:id="btnActiver" mnemonicParsing="false" onAction="#onActivate" text="Activer" />
      <Label fx:id="lblTache1" text="Label" />
      <Button fx:id="btnInterromre" mnemonicParsing="false" onAction="#onCancel" text="Interrompre" />
      <Label fx:id="lblTache2" text="Label" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
